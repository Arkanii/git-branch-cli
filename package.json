{
    "name": "@arkanii/git-branch-cli",
    "version": "1.0.0",
    "description": "A client for creating git branch easily with pattern.",
    "engines": {
        "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Arkanii/git-branch-cli.git"
    },
    "author": "Théo Frison (Arkanii)",
    "license": "MIT",
    "bin": {
        "git-branch": "src/cli.js"
    },
    "files": [
        "src"
    ],
    "type": "module",
    "bugs": {
        "url": "https://github.com/Arkanii/git-branch-cli/issues"
    },
    "homepage": "https://github.com/Arkanii/git-branch-cli#readme",
    "keywords": [
        "cli",
        "git",
        "branch"
    ],
    "dependencies": {
        "execa": "^5.1.1",
        "fuzzy": "^0.1.3",
        "inquirer": "^8.2.0",
        "inquirer-autocomplete-prompt": "^1.4.0",
        "load-json-file": "^7.0.1",
        "meow": "^10.1.1",
        "slugify": "^1.6.1"
    },
    "scripts": {},
    "devDependencies": {},
    "git-branch-cli": [
        {
            "name": "type",
            "message": "De quel type est votre branche ?",
            "type": "autocomplete",
            "choices": [
                "feature",
                "bug"
            ],
            "characterAfter": "/"
        },
        {
            "name": "name",
            "message": "Quel est le nom de la fonctionnalité ?"
        }
    ]
}
